{
  "name": "cli-engine-example",
  "description": "example CLI for use with cli-engine",
  "version": "1.4.0",
  "author": "Jeff Dickey @dickeyxxx",
  "bin": {
    "cli-engine-example": "bin/run"
  },
  "bugs": "https://github.com/heroku/cli-engine-example/issues",
  "cli-engine": {
    "bin": "cli-engine",
    "commands": "./lib/commands",
    "userPluginsEnabled": true,
    "hooks": {
      "init": "./lib/hooks/init"
    },
    "s3": {
      "host": "cli-assets.heroku.com"
    },
    "corePlugins": [
      "cli-engine-example-plugin"
    ]
  },
  "dependencies": {
    "@cli-engine/command": "^11.0.0-beta.4",
    "@cli-engine/config": "^5.0.0-beta.5",
    "@cli-engine/engine": "^6.1.4",
    "@cli-engine/util": "^1.0.5",
    "cli-engine": "^5.0.13",
    "cli-engine-example-plugin": "^3.0.5",
    "cli-flags": "^2.0.4"
  },
  "devDependencies": {
    "@types/node": "8.5.2",
    "jest": "22.0.4",
    "ts-jest": "^22.0.0",
    "typescript": "2.6.2"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "./"
  },
  "homepage": "https://github.com/heroku/cli-engine-example",
  "keywords": [
    "cli-engine-example"
  ],
  "license": "ISC",
  "repository": "https://github.com/heroku/cli-engine-example",
  "scripts": {
    "posttest": "posttest",
    "precommit": "precommit",
    "prepare": "cli-engine-util",
    "test": "jest"
  }
}

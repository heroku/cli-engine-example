{
  "name": "cli-engine-example",
  "description": "example CLI for use with cli-engine",
  "version": "1.4.0",
  "author": "Jeff Dickey @dickeyxxx",
  "bin": {
    "cli-engine-example": "bin/run"
  },
  "bugs": "https://github.com/heroku/cli-engine-example/issues",
  "cli-engine": {
    "bin": "cli-engine",
    "commands": "./lib/commands",
    "userPlugins": true,
    "hooks": {
      "init": "./lib/hooks/init",
      "plugins:parse": "./lib/hooks/plugins/parse/legacy"
    },
    "s3": {
      "host": "cli-assets.heroku.com"
    },
    "plugins": [
      "cli-engine-example-plugin"
    ]
  },
  "dependencies": {
    "cli-engine": "^4.6.21",
    "cli-engine-example-plugin": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "8.5.1",
    "jest": "21.2.1",
    "typescript": "2.6.2"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "./"
  },
  "homepage": "https://github.com/heroku/cli-engine-example",
  "keywords": [
    "cli-engine-example"
  ],
  "license": "ISC",
  "repository": "https://github.com/heroku/cli-engine-example",
  "scripts": {
    "test": "jest"
  }
}
